    <title>信度与效度的关系</title>
    <link href="http://yoursite.com/wiki/%E4%BF%A1%E5%BA%A6%E4%B8%8E%E6%95%88%E5%BA%A6%E7%9A%84%E5%85%B3%E7%B3%BB/"/>
